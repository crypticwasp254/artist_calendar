{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "indent": "off",
        "semi": "off",
        "import/extensions": "off",
        "no-underscore-dangle": "off",
        "max-classes-per-file": "off",
        "no-undef": "off",
        "class-methods-use-this": "off",
        "no-param-reassign": "off",
        "no-use-before-define": "off",
        "no-new": "off"
    }
}